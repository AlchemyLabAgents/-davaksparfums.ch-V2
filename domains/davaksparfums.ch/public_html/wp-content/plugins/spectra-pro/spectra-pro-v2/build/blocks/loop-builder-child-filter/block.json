{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra-pro/loop-builder-child-filter",
  "title": "Filter",
  "category": "spectra-pro-v2",
  "ancestor": [
    "spectra-pro/loop-builder"
  ],
  "allowedBlocks": [
    "spectra-pro/loop-builder-child-filter-button",
    "spectra-pro/loop-builder-child-filter-select",
    "spectra-pro/loop-builder-child-filter-checkbox"
  ],
  "description": "Loop Builder - Category filter allows user to filter posts in the loop.",
  "textdomain": "spectra-pro",
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "layout": {
      "type": "object",
      "default": {
        "type": "flex",
        "justifyContent": "left",
        "flexWrap": "nowrap"
      }
    },
    "filterType": {
      "type": "string",
      "default": "select"
    },
    "showPostCount": {
      "type": "boolean",
      "default": false
    },
    "showEmptyTaxonomy": {
      "type": "boolean",
      "default": false
    },
    "showChildren": {
      "type": "boolean",
      "default": false
    },
    "filterMode": {
      "type": "string",
      "default": "include"
    },
    "taxonomyType": {
      "type": "string",
      "default": "category"
    },
    "tagTaxonomyInclude": {
      "type": "array",
      "default": []
    },
    "tagTaxonomyExclude": {
      "type": "array",
      "default": []
    },
    "catTaxonomyInclude": {
      "type": "array",
      "default": []
    },
    "catTaxonomyExclude": {
      "type": "array",
      "default": []
    },
    "placeholder": {
      "type": "string"
    },
    "placeholderColor": {
      "type": "string"
    },
    "placeholderFocusColor": {
      "type": "string"
    },
    "textColor": {
      "type": "string"
    },
    "textColorFocus": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "string"
    },
    "backgroundColorFocus": {
      "type": "string"
    },
    "backgroundGradient": {
      "type": "string"
    },
    "backgroundGradientFocus": {
      "type": "string"
    },
    "itemsGap": {
      "type": "string"
    },
    "labelCheckboxGap": {
      "type": "string"
    }
  },
  "usesContext": [
    "spectra-pro/loop-builder/queryId",
    "spectra-pro/loop-builder/query"
  ],
  "providesContext": {
    "spectra-pro/loop-builder/queryId": "spectra-pro/loop-builder/queryId",
    "spectra-pro/loop-builder-child-filter/showPostCount": "showPostCount",
    "spectra-pro/loop-builder-child-filter/showEmptyTaxonomy": "showEmptyTaxonomy",
    "spectra-pro/loop-builder-child-filter/showChildren": "showChildren",
    "spectra-pro/loop-builder-child-filter/filterMode": "filterMode",
    "spectra-pro/loop-builder-child-filter/taxonomyType": "taxonomyType",
    "spectra-pro/loop-builder-child-filter/tagTaxonomyInclude": "tagTaxonomyInclude",
    "spectra-pro/loop-builder-child-filter/tagTaxonomyExclude": "tagTaxonomyExclude",
    "spectra-pro/loop-builder-child-filter/catTaxonomyInclude": "catTaxonomyInclude",
    "spectra-pro/loop-builder-child-filter/catTaxonomyExclude": "catTaxonomyExclude",
    "spectra-pro/loop-builder-child-filter/filterType": "filterType"
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "supports": {
    "align": true,
    "html": false,
    "shadow": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "style": true,
        "shadow": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "layout": {
      "allowSwitching": false,
      "allowInheriting": false,
      "default": {
        "type": "flex",
        "justifyContent": "left",
        "flexWrap": "nowrap"
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}