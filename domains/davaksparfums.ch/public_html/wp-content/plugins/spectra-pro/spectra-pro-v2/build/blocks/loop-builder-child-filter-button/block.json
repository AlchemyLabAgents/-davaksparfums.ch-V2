{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra-pro/loop-builder-child-filter-button",
  "version": "2.0.0",
  "title": "Button",
  "category": "spectra-pro-v2",
  "description": "A button that shows taxonomies from the Loop Builder block. It can be used to create a filterable navigation for the Loop Builder block.",
  "textdomain": "spectra-pro",
  "parent": [
    "spectra-pro/loop-builder-child-filter"
  ],
  "usesContext": [
    "spectra-pro/loop-builder/queryId",
    "spectra-pro/loop-builder-child-filter/showPostCount",
    "spectra-pro/loop-builder-child-filter/showEmptyTaxonomy",
    "spectra-pro/loop-builder-child-filter/showChildren",
    "spectra-pro/loop-builder-child-filter/filterMode",
    "spectra-pro/loop-builder-child-filter/taxonomyType",
    "spectra-pro/loop-builder-child-filter/tagTaxonomyInclude",
    "spectra-pro/loop-builder-child-filter/tagTaxonomyExclude",
    "spectra-pro/loop-builder-child-filter/catTaxonomyInclude",
    "spectra-pro/loop-builder-child-filter/catTaxonomyExclude",
    "spectra-pro/loop-builder-child-filter/filterType"
  ],
  "supports": {
    "inserter": false,
    "lock": false,
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "radius": true,
      "style": true,
      "shadow": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true,
        "radius": true,
        "style": true,
        "shadow": true
      }
    },
    "html": false,
    "align": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "__experimentalFontFamily": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalWritingMode": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontFamily": true
      }
    },
    "shadow": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "style": [
    "file:./style-index.css",
    "spectra-blocks-common"
  ],
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "text": {
      "type": "string"
    },
    "textColor": {
      "type": "string"
    },
    "textColorActive": {
      "type": "string"
    },
    "textColorHover": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "string"
    },
    "backgroundColorActive": {
      "type": "string"
    },
    "backgroundColorHover": {
      "type": "string"
    },
    "backgroundGradient": {
      "type": "string"
    },
    "backgroundGradientActive": {
      "type": "string"
    },
    "backgroundGradientHover": {
      "type": "string"
    }
  }
}