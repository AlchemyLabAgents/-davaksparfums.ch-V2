{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "spectra-pro/register-confirm-password",
  "version": "2.0.0",
  "title": "Register Confirm Password Field",
  "category": "spectra-pro-v2",
  "description": "Confirm password field specifically designed for registration forms.",
  "keywords": [
    "register",
    "confirm",
    "password",
    "field",
    "form"
  ],
  "textdomain": "spectra-pro",
  "parent": [
    "spectra-pro/register"
  ],
  "supports": {
    "interactivity": true,
    "html": false,
    "anchor": true,
    "multiple": false,
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": false,
        "padding": false
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": false,
        "radius": false,
        "style": false,
        "width": false
      }
    },
    "color": {
      "gradients": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": false
      }
    },
    "shadow": true
  },
  "example": {
    "attributes": {
      "isPreview": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "editorStyle": "file:./index.css",
  "usesContext": [
    "spectra-pro/form/formId",
    "spectra-pro/form/formType",
    "spectra-pro/form/usernameLabel",
    "spectra-pro/form/usernamePlaceholder",
    "spectra-pro/form/passwordLabel",
    "spectra-pro/form/passwordPlaceholder",
    "spectra-pro/form/showLabels",
    "spectra-pro/form/showIcons",
    "spectra-pro/form/labelColor",
    "spectra-pro/form/labelColorHover",
    "spectra-pro/form/inputTextColor",
    "spectra-pro/form/inputTextColorHover",
    "spectra-pro/form/inputBgColor",
    "spectra-pro/form/inputBgColorHover",
    "spectra-pro/form/iconColor",
    "spectra-pro/form/formWidth",
    "spectra-pro/form/overallAlignment",
    "spectra-pro/form/alignLoginBtn",
    "spectra-pro/form/inputPlaceholderColor",
    "spectra-pro/form/inputTextColorActive",
    "spectra-pro/form/inputBgColorActive",
    "spectra-pro/form/inputBorderColorActive",
    "spectra-pro/form/fieldsIconColor",
    "spectra-pro/form/fieldsBackground",
    "spectra-pro/form/fieldsBackgroundHover",
    "spectra-pro/form/fieldsBackgroundActive",
    "spectra-pro/form/placeholderColor",
    "spectra-pro/form/placeholderColorHover",
    "spectra-pro/form/placeholderColorActive",
    "spectra-pro/form/fieldsColor",
    "spectra-pro/form/demoFieldErrorMessage",
    "spectra-pro/form/messageInvalidEmailError",
    "spectra-pro/form/messagePasswordConfirmError"
  ],
  "providesContext": {
    "spectra-pro/form-input/fieldType": "fieldType",
    "spectra-pro/form-input/fieldLabel": "fieldLabel",
    "spectra-pro/form-input/fieldPlaceholder": "fieldPlaceholder",
    "spectra-pro/form-input/fieldIcon": "fieldIcon",
    "spectra-pro/form-input/isRequired": "isRequired",
    "spectra-pro/form-input/fieldId": "fieldId"
  },
  "template": [
    [
      "spectra-pro/form-input-label"
    ],
    [
      "spectra-pro/form-field-wrapper",
      {},
      [
        [
          "spectra-pro/form-icon"
        ],
        [
          "spectra-pro/form-input-field"
        ]
      ]
    ]
  ],
  "allowedBlocks": [
    "spectra-pro/form-input-label",
    "spectra-pro/form-icon",
    "spectra-pro/form-input-field",
    "spectra-pro/form-field-wrapper"
  ],
  "attributes": {
    "isPreview": {
      "type": "boolean",
      "default": false
    },
    "fieldType": {
      "type": "string",
      "default": "confirm_password"
    },
    "fieldLabel": {
      "type": "string",
      "default": "Confirm Password"
    },
    "fieldPlaceholder": {
      "type": "string",
      "default": "Re-enter password"
    },
    "fieldIcon": {
      "type": "string",
      "default": "lock"
    },
    "isRequired": {
      "type": "boolean",
      "default": true
    },
    "fieldId": {
      "type": "string",
      "default": ""
    },
    "showLabel": {
      "type": "boolean"
    },
    "iconPosition": {
      "type": "string",
      "default": "left",
      "enum": [
        "left",
        "right"
      ]
    },
    "textColorHover": {
      "type": "string"
    },
    "backgroundColorHover": {
      "type": "string"
    },
    "backgroundGradientHover": {
      "type": "string"
    },
    "inputFieldBgColor": {
      "type": "string"
    },
    "inputFieldBgColorHover": {
      "type": "string"
    },
    "inputFieldBgColorActive": {
      "type": "string"
    },
    "errorColor": {
      "type": "string"
    },
    "errorColorHover": {
      "type": "string"
    },
    "formBackgroundColor": {
      "type": "string"
    }
  },
  "render": "file:./controller.php"
}